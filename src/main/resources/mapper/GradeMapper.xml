<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yutyu.mapper.GradeMapper">

    <resultMap id="BaseResultMap" type="com.yutyu.domain.Grade">
            <id property="id" column="id" jdbcType="INTEGER"/>
            <result property="year" column="year" jdbcType="DATE"/>
            <result property="ranking" column="ranking" jdbcType="INTEGER"/>
            <result property="state" column="state" jdbcType="VARCHAR"/>
            <result property="type" column="type" jdbcType="INTEGER"/>
    </resultMap>

    <sql id="Base_Column_List">
        id,year,ranking,
        state,type
    </sql>
</mapper>
